msgctxt "Coastal Hub.Server.Factories.EmailFactory"
msgid "TestEmail.template"
msgstr "<div>   <h1>     <img style=\"width:100px; vertical-align: middle;\" src=\"https://coastalhub.firstcitizens.org/images/logo.png\" alt=\"Coastal Hub\"/> Coastal Hub   </h1>   <p>Hello {user},</p>   <p>     This is a <strong>TEST</strong> email. If you are using Coastal Hub please <a href=\"https://github.com/enkodellc/Coastal Hub\">star the repo on Github</a>.     We are looking for more contributors to grow the project. There are several areas of improvement needed and if you are using the repo in your project please submit your PR's or suggestions.   </p>   <p>     Join our <a href=\"https://gitter.im/Coastal Hub/community\">chat on Gitter</a> to disucss future project features, timelines and to get involved.   </p>   <p>The email was on {testDate}.</p>   <p>     <br />   </p>   <p>Regards,</p>   <p>Coastal Hub Email Template</p> </div>"

msgctxt "Coastal Hub.Server.Factories.EmailFactory"
msgid "PlainTextTestEmail.template"
msgstr "Hello, This email was sent using the plain text test email template. The test was on {date}.  Regards, Blazor BoilerplateTemplate"

msgctxt "Coastal Hub.Server.Factories.EmailFactory"
msgid "PasswordReset.template"
msgstr "<div>   <h1>     <img style=\"width:100px; vertical-align: middle;\" src=\"https://coastalhub.firstcitizens.org/images/logo.png\" alt=\"Coastal Hub\"/> Coastal Hub   </h1>   <h3>Dear {userName},</h3>   <p>Your password has been successfully reset, you can now login with your new password.</p>   <br/>   <div>     Cheers!   </div> </div>"

msgctxt "Coastal Hub.Server.Factories.EmailFactory"
msgid "NewUserNotificationEmail.template"
msgstr "<div>   <h1>     <img style=\"width:100px; vertical-align: middle;\" src=\"https://coastalhub.firstcitizens.org/images/logo.png\" alt=\"Coastal Hub\"/> Coastal Hub   </h1>   <p>Hello {name},</p>   <p>     A new user has been added as a new user to <a href=\"https://coastalhub.firstcitizens.org\" target=\"_blank\">Coastal Hub.com</a>   </p>   <p>     Created by {creator}<br/>     Company: {company}<br/>     Roles: {roles}<br/>     Name: {name}<br/>     User Name: {userName}<br/>>     Email: {email}<br/>   </p>   <p>     <br />   </p>   <p>Regards,</p>   <p>Coastal Hub Team</p> </div>"

msgctxt "Coastal Hub.Server.Factories.EmailFactory"
msgid "NewUserEmail.template"
msgstr "<div>   <h1>     <img style=\"width:100px; vertical-align: middle;\" src=\"https://coastalhub.firstcitizens.org/images/logo.png\" alt=\"Coastal Hub\"/> Coastal Hub   </h1>   <p>Hello {fullName},</p>   <p>     You have been added as a new user to <a href=\"https://coastalhub.firstcitizens.org\" target=\"_blank\">https://coastalhub.firstcitizens.org</a> Below are your new credentials.   </p>   <p>     User Name: {userName}<br />     Email: {email}<br />     Password: {password}   </p>   <ul>     <li>       Please save this email to refer to your login credentials.     </li>     <li>       You can log in using the <a href=\"https://coastalhub.firstcitizens.org/account/login\">link provided here.</a>     </li>   </ul>   <p>     <br />   </p>   <p>Thank you, and welcome aboard the Coastal Hub</p> </div>"

msgctxt "Coastal Hub.Server.Factories.EmailFactory"
msgid "NewUserConfirmationEmail.template"
msgstr "<div>   <h1>     <img style=\"width:100px; vertical-align: middle;\" src=\"https://coastalhub.firstcitizens.org/images/logo.png\" alt=\"Coastal Hub\"/> Coastal Hub   </h1>   <p>Hello {userName},</p>   <p>     Please confirm your account by: <a href='{callbackUrl}'>clicking this link.</a>   </p>   <p>     <strong>UserId:</strong> {userId}<br />     <strong>Token:</strong> {token}<br/>   </p>   <p>Thank you, and welcome to the Coastal Hub</p> </div>"

msgctxt "Coastal Hub.Server.Factories.EmailFactory"
msgid "ForgotPassword.template"
msgstr "<div>   <h1>     <img style=\"width:100px; vertical-align: middle;\" src=\"https://coastalhub.firstcitizens.org/images/logo.png\" alt=\"Coastal Hub\"/> Coastal Hub   </h1>   <h3>Dear {name},</h3>   <p>     You requested for a password reset, kindly use this <a href=\"{callbackUrl}\">link</a> to reset your password.   </p>   <br/>   <p>Cheers!</p>  </div>"

